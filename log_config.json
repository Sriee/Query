{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default":{
          "format": "%(asctime)s:%(levelname)s: %(message)s",
          "datefmt": "[%m-%d-%Y][%H:%M]"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "default",
            "filename": "log/controller.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "encoding": "utf8"
        }
    },
    "loggers": {
        "main": {
            "level": "DEBUG",
            "handlers": ["file"],
            "propagate": "no"
        }
    },
    "root": {
        "level": "INFO"
    }
}
